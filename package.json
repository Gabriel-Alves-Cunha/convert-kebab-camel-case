{
	"name": "convert-kebab-camel-case",
	"description": "Convert kebab to camel case string and vice-versa.",
	"author": "Gabriel Alves Cunha <gabriel925486@outlook.com>",
	"displayName": "convert-kebab-camel-case",
	"version": "0.0.1",
	"type": "commonjs",
	"license": "MIT",
	"engines": {
		"vscode": "^1.77.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "convert-kebab-camel-case.camelToKebab",
				"title": "Camel to kebab"
			},
			{
				"command": "convert-kebab-camel-case.kebabToCamel",
				"title": "Kebab to camel"
			}
		]
	},
	"scripts": {
		"lint": "rome check . && eslint src --ext ts && tsc",
		"vscode:prepublish": "yarn build",
		"fmt": "rome format . --write",
		"compile": "yarn build",
		"prepack": "yarn build",
		"build": "vite build",
		"test": "vitest",
		"dev": "vite"
	},
	"devDependencies": {
		"@types/node": "18.16.0",
		"@types/vscode": "^1.77.0",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"@vscode/test-electron": "^2.3.0",
		"eslint": "^8.39.0",
		"rome": "^12.0.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.1",
		"vitest": "^0.30.1"
	}
}
